{
  "name": "yarn_js",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build-css": "sass src/sass:dist/css/ --style=compressed --watch && postcss dist/css/style.css --use autoprefixer --dir dist/css/",
    "build-js": "webpack --config webpack.config.mjs --watch",
    "build": "concurrently \"npm run build-css\" \"npm run build-js\""
  },
  "volta": {
    "node": "20.9.0",
    "yarn": "4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "sass": "^1.69.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": [
    "last 2 versions",
    "> 0.25%, not dead"
  ],
  "dependencies": {
    "lottie-web": "^5.12.2"
  }
}
